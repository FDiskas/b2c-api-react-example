  "jest": {
    "setupFiles": [
      "<rootDir>/test/test-shim.js",
      "<rootDir>/test/test-setup.js"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/unit/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "cpy:local:serve": "cpy local_serve.env ./ --rename=.env",
    "cpy:local:dev": "cpy local_dev.env ./ --rename=.env",
    "cpy:local:test": "cpy local_test.env ./ --rename=.env",
    "cpy:local:docker": "cpy local_docker.env ./ --rename=.env",
    "cpy:local:serve_windows": "cpy local_windows.env ./ --rename=.env",
    "cpy:prod": "cpy prod.env ./ --rename=.env",
    "build:web": "node --max_old_space_size=4096 node_modules/.bin/webpack-cli --config ./web-webpack-config.js --progress --profile --colors",
    "build:web:prod": "cross-env NODE_ENV=production npm run build:web",
    "dist:dev": "npm run cpy:local:dev && npm run build:web",
    "start:docker": "npm run cpy:local:docker && sudo docker-compose up --build",
    "dist:prod": "npm run cpy:prod && npm run build:web:prod",
    "dist": "npm run build:web:prod",
    "dev": "cross-env NODE_ENV=webpack-dev-server node server.js",
    "serve": "npm run cpy:local:serve && npm run dev",
    "serve:windows": "npm run cpy:local:serve_windows && npm run dev",
    "start": "node ./server.js",
    "pm2:start": "pm2 start ./server.js",
    "pm2:stop": "pm2 stop ./server.js",
    "pm2:restart": "pm2 restart ./server.js",
    "unit": "npm run cpy:local:test && jest --runInBand",
    "coverage": "jest --runInBand --coverage",
    "test": "npm run unit",
    "analyze": "npm run cpy:prod && cross-env NODE_ENV=bundle-analyzer webpack -p --config ./web-webpack-config.js --progress --profile --colors --profile --json > ./stats.json && webpack-bundle-analyzer ./stats.json",
    "test:watch": "jest --watch"
  }
